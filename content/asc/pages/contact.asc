{:title "Contact"
 :layout :page}
++++
<form name="contact
      data-netlify="true"
      data-netlify-honeypot="bot-field"">
      <label>Name
        <input name="name" type="text" />
      </label>
      <label>E-mail
        <input name="email" type="email" required placeholder="johnsnow@example.com"/>
      </label>
      <label>Message
        <input name="message" type="textarea" required placeholder="Please input your message!"/>
      </label>
      <button type="submit">Submit</button>
</form>
++++
